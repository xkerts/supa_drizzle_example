{
  "name": "supadrizzle_example",
  "version": "1.0.0",
  "description": "A simple express, supabase and drizzle example",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "introspect": "drizzle-kit introspect:pg",
    "generate": "drizzle-kit generate:pg",
    "migrate": "tsx src/migrate.ts"
  },
  "author": "xkerts",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "drizzle-kit": "^0.20.14",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "postgres": "^3.4.3"
  }
}